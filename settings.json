{
  "settingsSync.ignoredExtensions": [
    "piesarentsquare.smooth-dark"
  ],
  "terminal.integrated.fontFamily": "FiraCode NF",
  // editor
  "editor.minimap.enabled": false,
  "editor.fontFamily": "FiraCode NF",
  "editor.codeLensFontFamily": "FiraCode NF",
  "editor.guides.bracketPairs": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.wordBasedSuggestions": true,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.wordWrap": "on",
  "editor.inlayHints.fontFamily": "FiraCode NF",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  // fin editor
  "files.autoSave": "afterDelay",
  "window.zoomLevel": 2,
  "eslint.enable": true,
  "eslint.run": "onSave",
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "php-intellisense.trace.server": "verbose",
  "php.validate.enable": true,
  "php.validate.run": "onType",
  "php.suggest.basic": true,
  "[php]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "zobo.php-intellisense"
  },
  "intelephense.phpdoc.textFormat": "snippet",
  "[javascript]": {
    "editor.formatOnSave": true,
    // "editor.defaultFormatter": "vscode.typescript-language-features"
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    // "editor.defaultFormatter": "vscode.typescript-language-features"
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "eslint.format.enable": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  // nuevos
  "javascript.format.insertSpaceAfterCommaDelimiter": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.semicolons": "insert",
  "javascript.format.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.autoClosingTags": true,
  "javascript.validate.enable": true,
  "javascript.preferences.jsxAttributeCompletionStyle": "auto",
  // typescript
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.semicolons": "insert",
  "typescript.format.enable": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.autoClosingTags": true,
  "typescript.validate.enable": true,
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  // fin TS
  // prettier
  "prettier.tabWidth": 2,
  "prettier.jsxSingleQuote": true,
  "prettier.htmlWhitespaceSensitivity": "strict",
  "prettier.arrowParens": "always",
  "prettier.jsxBracketSameLine": true,
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 100,
  "html.format.unformatted": "wbr",
  "html.format.contentUnformatted": "pre,code,textarea",
  "html.format.maxPreserveNewLines": 100,
  "html.autoClosingTags": true,
  "html.format.enable": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "SAVEDB",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/**",
    "**/vendor/**",
    "**/bower_components/",
    "**/dist/**"
  ],
  "git.confirmSync": false,
  "editor.renderWhitespace": "all",
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    // ayu light
    "[Default Light+]": {
      "textMateRules": [
        {
          "name": "Comment",
          "scope": [
            "comment"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#787b8099"
          }
        },
        {
          "name": "Number",
          "scope": [
            "constant.numeric"
          ],
          "settings": {
            "foreground": "#a37acc"
          }
        },
        {
          "name": "Variable",
          "scope": [
            "variable",
            "variable.parameter.function-call"
          ],
          "settings": {
            "foreground": "#F0884B"
          }
        },
        {
          "name": "Member Variable",
          "scope": [
            "variable.member"
          ],
          "settings": {
            "foreground": "#f07171"
          }
        },
        {
          "name": "Language variable",
          "scope": [
            "variable.language"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#55b4d4"
          }
        },
        {
          "name": "Number",
          "scope": [
            "constant.numeric"
          ],
          "settings": {
            "foreground": "#a37acc"
          }
        },
        {
          "name": "String",
          "scope": [
            "string",
            "constant.other.symbol"
          ],
          "settings": {
            "foreground": "#86b300"
          }
        },
        {
          "name": "Function name",
          "scope": [
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#f2ae49"
          }
        },
        {
          "name": "Function arguments",
          "scope": [
            "variable.parameter",
            "meta.parameter"
          ],
          "settings": {
            "foreground": "#a37acc"
          }
        },
        {
          "name": "Function call",
          "scope": [
            "variable.function",
            "variable.annotation",
            "meta.function-call.generic",
            "support.function.go"
          ],
          "settings": {
            "foreground": "#f2ae49"
          }
        },
        {
          "name": "Library function",
          "scope": [
            "support.function",
            "support.macro"
          ],
          "settings": {
            "foreground": "#f07171"
          }
        },
        {
          "name": "Imports and packages",
          "scope": [
            "entity.name.import",
            "entity.name.package"
          ],
          "settings": {
            "foreground": "#86b300"
          }
        },
        {
          "name": "Java primitive variable types",
          "scope": [
            "source.java storage.type.primitive"
          ],
          "settings": {
            "foreground": "#222"
          }
        }
      ],
      "types": "#64aa9f"
    }
  },
